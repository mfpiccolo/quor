<% content_for :tabbar_title do %>
  New Project
<% end %>

<div class="form-panel clearfix">
  <ul class="breadcrumbs">
    <li><%= link_to "Models", models_path %></li>
    <li class="current"><a href="#">New</a></li>
  </ul>
  <%= simple_form_for @model, role: "form" do |f| %>
    <div class="form-group">
      <% Model.data_keys(@model.otype).each do |k| %>
        <%= f.input k.to_sym %>
      <% end %>
    </div>

    <%= f.submit %>
  <% end %>
</div>

