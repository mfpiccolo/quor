<h2><%= @models.first.otype %></h2>
<% unless @models.first.data.empty? %>
  <table class="table">
    <tr>
      <th>ID</th>
      <% @models.last.data.keys.each do |k| %>
        <th><%= k %></th>
      <% end %>
      <th></th>
    </tr>
    <% @models.each do |m| %>
      <tr>
        <td><%= m.id %></td>
        <% m.data.each_pair do |k,v| %>
          <td><%= best_in_place m, k.to_sym, :type => :input %></td>
        <% end %>
      <tr>
    <% end %>
  </table>
<% end %>
